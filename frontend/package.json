{
  "name": "subcellular-experiment",
  "version": "0.0.4",
  "description": "Subcellular experiment application",
  "author": "NSE(Neuroscientific Software Engineering) <bbp-ou-nse@groupes.epfl.ch> (http://bluebrain.epfl.ch)",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "prettier": "npx prettier --write src",
    "lint": "npx prettier --check src && vue-cli-service lint --no-fix && npx tsc --noEmit"
  },
  "dependencies": {
    "@msgpack/msgpack": "^1.11.0",
    "@sentry/browser": "^5.12.1",
    "@sentry/integrations": "^5.12.0",
    "@types/plotly.js": "^1.54.2",
    "@types/uuid": "^8.3.0",
    "ajv": "^6.11.0",
    "chroma-js": "^2.1.0",
    "codemirror": "^5.51.0",
    "core-js": "^3.6.4",
    "cytoscape": "^3.19.0",
    "cytoscape-cose-bilkent": "^4.1.0",
    "distinct-colors": "^2.1.0",
    "file-saver": "^2.0.2",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "papaparse": "^5.1.1",
    "plotly.js-basic-dist": "^1.52.2",
    "sass": "^1.27.0",
    "simple-web-worker": "^1.2.0",
    "superscript-text": "^1.0.0",
    "three": "^0.113.2",
    "toggle-fullscreen": "^0.4.2",
    "uuid": "^3.4.0",
    "uuidv4": "^6.2.6",
    "view-design": "^4.1.2",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-codemirror": "^4.0.6",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli": "^4.5.8",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.0.3",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.1.2",
    "prettier": "^2.1.2",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "airbnb-base",
      "prettier",
      "@vue/typescript"
    ],
    "rules": {
      "max-classes-per-file": "off",
      "spaced-comment": "off",
      "no-underscore-dangle": "off",
      "no-param-reassign": "off",
      "no-trailing-spaces": "error",
      "consistent-return": "off",
      "no-plusplus": "off",
      "import/no-cycle": "off",
      "lines-between-class-members": "off",
      "no-console": "off",
      "no-shadow": "off",
      "prefer-destructuring": "off",
      "no-continue": "off",
      "no-restricted-syntax": "off",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "settings": {
      "import/resolver": {
        "alias": [
          [
            "@",
            "./frontend/src"
          ]
        ]
      }
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "vueIndentScriptAndStyle": true
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "contributors": [
    "pavlo.getta@epfl.ch"
  ],
  "homepage": "https://bluebrain.epfl.ch",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://bbpcode.epfl.ch/browse/code/nse/subcellular-experiment-app/",
    "issuesurl": "NA"
  }
}
