{
  "name": "subcellular-experiment",
  "version": "0.0.4",
  "description": "Subcellular experiment application",
  "author": "NSE(Neuroscientific Software Engineering) <bbp-ou-nse@groupes.epfl.ch> (http://bluebrain.epfl.ch)",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@msgpack/msgpack": "^1.11.0",
    "@sentry/browser": "^5.12.1",
    "@sentry/integrations": "^5.12.0",
    "ajv": "^6.11.0",
    "chroma-js": "^2.1.0",
    "codemirror": "^5.51.0",
    "core-js": "^3.6.4",
    "distinct-colors": "^2.1.0",
    "file-saver": "^2.0.2",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "papaparse": "^5.1.1",
    "plotly.js-basic-dist": "^1.52.2",
    "sass": "^1.27.0",
    "simple-web-worker": "^1.2.0",
    "superscript-text": "^1.0.0",
    "three": "^0.113.2",
    "toggle-fullscreen": "^0.4.2",
    "uuid": "^3.4.0",
    "view-design": "^4.1.2",
    "vue": "^2.6.11",
    "vue-codemirror": "^4.0.6",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^6.1.2",
    "prettier": "^2.1.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "indent": "off",
      "vue/script-indent": [
        "error",
        2,
        {
          "baseIndent": 1
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "settings": {
      "import/resolver": {
        "alias": [
          [
            "@",
            "./frontend/src"
          ]
        ]
      }
    }
  },
  "prettier": {
    "semi": false,
    "printWidth": 120,
    "singleQuote": true
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "contributors": [
    "pavlo.getta@epfl.ch"
  ],
  "homepage": "https://bluebrain.epfl.ch",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://bbpcode.epfl.ch/browse/code/nse/subcellular-experiment-app/",
    "issuesurl": "NA"
  }
}
